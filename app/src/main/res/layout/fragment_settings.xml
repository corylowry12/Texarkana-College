<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:tag="settings"
    android:id="@+id/coordinatorLayoutSettings">

    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/appBarLayoutGrades"
        android:layout_width="match_parent"
        android:layout_height="128dp"
        android:fitsSystemWindows="true"
        app:liftOnScroll="true"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <com.google.android.material.appbar.CollapsingToolbarLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:expandedTitleMarginBottom="28dp"
            app:layout_scrollFlags="scroll|exitUntilCollapsed|snap">

            <com.google.android.material.appbar.MaterialToolbar
                android:id="@+id/materialToolBarGrades"
                style="@style/Widget.MaterialComponents.Toolbar.Primary"
                android:layout_width="match_parent"
                android:layout_height="?attr/actionBarSize"
                android:background="@android:color/transparent"
                android:elevation="0dp"
                app:layout_collapseMode="pin"
                app:title="Settings"
                app:titleCentered="true" />
        </com.google.android.material.appbar.CollapsingToolbarLayout>
    </com.google.android.material.appbar.AppBarLayout>

    <androidx.core.widget.NestedScrollView
        android:id="@+id/nestedScrollViewSettings"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:overScrollMode="always"
        app:layout_behavior="@string/appbar_scrolling_view_behavior">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

    <com.google.android.material.card.MaterialCardView
        android:id="@+id/themeCardView"
        style="?attr/materialCardViewElevatedStyle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="5dp"
        app:cardCornerRadius="28dp"
        app:cardPreventCornerOverlap="false"
        app:cardUseCompatPadding="true"
        android:visibility="gone">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintAppearance"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:foreground="?attr/selectableItemBackground">

            <ImageView
                android:id="@+id/appearanceImage"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_marginStart="12dp"
                android:src="@drawable/ic_baseline_palette_24"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="ContentDescription" />

            <TextView
                android:id="@+id/themeHeading"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:paddingStart="12dp"
                android:paddingTop="4dp"
                android:paddingEnd="5dp"
                android:text="Appearance"
                android:textSize="18sp"
                app:layout_constraintEnd_toStartOf="@id/chevronImage"
                app:layout_constraintStart_toEndOf="@id/appearanceImage"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_below="@id/themeHeading"
                android:layout_alignStart="@id/themeHeading"
                android:paddingStart="12dp"
                android:paddingEnd="5dp"
                android:paddingBottom="4dp"
                android:text="Customize the appearance settings of the app"
                android:textSize="12sp"
                app:layout_constraintEnd_toStartOf="@id/chevronImage"
                app:layout_constraintStart_toEndOf="@id/appearanceImage"
                app:layout_constraintTop_toBottomOf="@id/themeHeading" />

            <ImageView
                android:id="@+id/chevronImage"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_marginStart="12dp"
                android:layout_marginEnd="10dp"
                android:src="@drawable/ic_baseline_chevron_right_24"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="ContentDescription" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </com.google.android.material.card.MaterialCardView>

    <com.google.android.material.card.MaterialCardView
        style="?attr/materialCardViewElevatedStyle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        app:cardCornerRadius="28dp"
        app:cardPreventCornerOverlap="false"
        app:cardUseCompatPadding="true"
        app:cardBackgroundColor="@color/cardViewLightBlue">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintCampusNews"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:foreground="?attr/selectableItemBackground">

            <ImageView
                android:id="@+id/appSettingsImage"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_marginStart="12dp"
                android:src="@drawable/ic_baseline_menu_book_24"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="ContentDescription" />

            <TextView
                android:id="@+id/appSettings"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:paddingStart="12dp"
                android:paddingTop="4dp"
                android:paddingEnd="5dp"
                android:text="Campus News"
                android:textSize="18sp"
                app:layout_constraintEnd_toStartOf="@id/appSettingsChevron"
                app:layout_constraintStart_toEndOf="@id/appSettingsImage"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_below="@id/appSettings"
                android:layout_alignStart="@id/appSettings"
                android:paddingStart="12dp"
                android:paddingEnd="5dp"
                android:paddingBottom="4dp"
                android:text="View news directly from the campus"
                android:textSize="12sp"
                app:layout_constraintEnd_toStartOf="@id/appSettingsChevron"
                app:layout_constraintStart_toEndOf="@id/appSettingsImage"
                app:layout_constraintTop_toBottomOf="@id/appSettings" />

            <ImageView
                android:id="@+id/appSettingsChevron"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_marginStart="12dp"
                android:layout_marginEnd="10dp"
                android:src="@drawable/ic_baseline_chevron_right_24"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="ContentDescription" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </com.google.android.material.card.MaterialCardView>

        <com.google.android.material.card.MaterialCardView
            android:id="@+id/communityBoardCardView"
            style="?attr/materialCardViewElevatedStyle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginTop="5dp"
            app:cardCornerRadius="28dp"
            app:cardPreventCornerOverlap="false"
            app:cardUseCompatPadding="true"
            app:cardBackgroundColor="@color/cardViewLightBlue">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/communityBoardConstraint"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:foreground="?attr/selectableItemBackground">

                <ImageView
                    android:id="@+id/communityBoardImage"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginStart="12dp"
                    android:src="@drawable/ic_baseline_people_alt_24"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="ContentDescription" />

                <TextView
                    android:id="@+id/communityBoardHeading"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:paddingStart="12dp"
                    android:paddingTop="4dp"
                    android:paddingEnd="5dp"
                    android:text="Community Board"
                    android:textSize="18sp"
                    app:layout_constraintEnd_toStartOf="@id/communityBoardChevron"
                    app:layout_constraintStart_toEndOf="@id/communityBoardImage"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/communityBoardHeading"
                    android:layout_alignStart="@id/communityBoardHeading"
                    android:paddingStart="12dp"
                    android:paddingEnd="5dp"
                    android:paddingBottom="4dp"
                    android:text="View changes in this update"
                    android:textSize="12sp"
                    app:layout_constraintEnd_toStartOf="@id/communityBoardChevron"
                    app:layout_constraintStart_toEndOf="@id/communityBoardImage"
                    app:layout_constraintTop_toBottomOf="@id/communityBoardHeading" />

                <ImageView
                    android:id="@+id/communityBoardChevron"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginStart="12dp"
                    android:layout_marginEnd="10dp"
                    android:src="@drawable/ic_baseline_chevron_right_24"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="ContentDescription" />
            </androidx.constraintlayout.widget.ConstraintLayout>
        </com.google.android.material.card.MaterialCardView>

    <com.google.android.material.card.MaterialCardView
        style="?attr/materialCardViewElevatedStyle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="5dp"
        app:cardCornerRadius="28dp"
        app:cardPreventCornerOverlap="false"
        app:cardUseCompatPadding="true"
        app:cardBackgroundColor="@color/cardViewLightBlue">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintPatchNotes"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:foreground="?attr/selectableItemBackground">

            <ImageView
                android:id="@+id/patchNotesImage"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_marginStart="12dp"
                android:src="@drawable/ic_baseline_update_24"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="ContentDescription" />

            <TextView
                android:id="@+id/patchNotesHeading"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:paddingStart="12dp"
                android:paddingTop="4dp"
                android:paddingEnd="5dp"
                android:text="Patch Notes"
                android:textSize="18sp"
                app:layout_constraintEnd_toStartOf="@id/patchNotesChevron"
                app:layout_constraintStart_toEndOf="@id/patchNotesImage"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_below="@id/patchNotesHeading"
                android:layout_alignStart="@id/patchNotesHeading"
                android:paddingStart="12dp"
                android:paddingEnd="5dp"
                android:paddingBottom="4dp"
                android:text="View changes in this update"
                android:textSize="12sp"
                app:layout_constraintEnd_toStartOf="@id/patchNotesChevron"
                app:layout_constraintStart_toEndOf="@id/patchNotesImage"
                app:layout_constraintTop_toBottomOf="@id/patchNotesHeading" />

            <ImageView
                android:id="@+id/patchNotesChevron"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_marginStart="12dp"
                android:layout_marginEnd="10dp"
                android:src="@drawable/ic_baseline_chevron_right_24"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="ContentDescription" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </com.google.android.material.card.MaterialCardView>

        <com.google.android.material.card.MaterialCardView
            style="?attr/materialCardViewElevatedStyle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="5dp"
            app:cardCornerRadius="28dp"
            app:cardPreventCornerOverlap="false"
            app:cardUseCompatPadding="true"
            android:visibility="gone">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintReasons"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:foreground="?attr/selectableItemBackground">

                <ImageView
                    android:id="@+id/reasonsImage"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginStart="12dp"
                    android:src="@drawable/ic_baseline_question_answer_24"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="ContentDescription" />

                <TextView
                    android:id="@+id/reasonsHeading"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:paddingStart="12dp"
                    android:paddingTop="4dp"
                    android:paddingEnd="5dp"
                    android:text="Why Choose TC"
                    android:textSize="18sp"
                    app:layout_constraintEnd_toStartOf="@id/reasonsChevron"
                    app:layout_constraintStart_toEndOf="@id/reasonsImage"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/reasonsHeading"
                    android:layout_alignStart="@id/reasonsHeading"
                    android:paddingStart="12dp"
                    android:paddingEnd="5dp"
                    android:paddingBottom="4dp"
                    android:text="View reasons to see why YOU might want to go to TC"
                    android:textSize="12sp"
                    app:layout_constraintEnd_toStartOf="@id/reasonsChevron"
                    app:layout_constraintStart_toEndOf="@id/reasonsImage"
                    app:layout_constraintTop_toBottomOf="@id/reasonsHeading" />

                <ImageView
                    android:id="@+id/reasonsChevron"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginStart="12dp"
                    android:layout_marginEnd="10dp"
                    android:src="@drawable/ic_baseline_chevron_right_24"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="ContentDescription" />
            </androidx.constraintlayout.widget.ConstraintLayout>
        </com.google.android.material.card.MaterialCardView>

        <com.google.android.material.card.MaterialCardView
            style="?attr/materialCardViewElevatedStyle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="5dp"
            app:cardCornerRadius="28dp"
            app:cardPreventCornerOverlap="false"
            app:cardUseCompatPadding="true"
            app:cardBackgroundColor="@color/cardViewLightBlue">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintCampusMap"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:foreground="?attr/selectableItemBackground">

                <ImageView
                    android:id="@+id/deletionImage"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginStart="12dp"
                    android:src="@drawable/ic_baseline_map_24"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="ContentDescription" />

                <TextView
                    android:id="@+id/campusMapHeading"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:paddingStart="12dp"
                    android:paddingTop="4dp"
                    android:paddingEnd="5dp"
                    android:text="Campus Map"
                    android:textSize="18sp"
                    app:layout_constraintEnd_toStartOf="@id/automaticChevron"
                    app:layout_constraintStart_toEndOf="@id/deletionImage"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/campusMapHeading"
                    android:layout_alignStart="@id/campusMapHeading"
                    android:paddingStart="12dp"
                    android:paddingEnd="5dp"
                    android:paddingBottom="4dp"
                    android:text="View the campus map"
                    android:textSize="12sp"
                    app:layout_constraintEnd_toStartOf="@id/automaticChevron"
                    app:layout_constraintStart_toEndOf="@id/deletionImage"
                    app:layout_constraintTop_toBottomOf="@id/campusMapHeading" />

                <ImageView
                    android:id="@+id/automaticChevron"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginStart="12dp"
                    android:layout_marginEnd="10dp"
                    android:src="@drawable/ic_baseline_chevron_right_24"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="ContentDescription" />
            </androidx.constraintlayout.widget.ConstraintLayout>
        </com.google.android.material.card.MaterialCardView>

        <com.google.android.material.card.MaterialCardView
            style="?attr/materialCardViewElevatedStyle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="5dp"
            app:cardCornerRadius="28dp"
            app:cardPreventCornerOverlap="false"
            app:cardUseCompatPadding="true"
            android:visibility="gone">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintAppNews"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:foreground="?attr/selectableItemBackground">

                <ImageView
                    android:id="@+id/appNewsImage"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginStart="12dp"
                    android:src="@drawable/ic_baseline_new_releases_24"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="ContentDescription" />

                <TextView
                    android:id="@+id/appNewsHeading"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:paddingStart="12dp"
                    android:paddingTop="4dp"
                    android:paddingEnd="5dp"
                    android:text="App News"
                    android:textSize="18sp"
                    app:layout_constraintEnd_toStartOf="@id/appNewsChevron"
                    app:layout_constraintStart_toEndOf="@id/appNewsImage"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/appNewsHeading"
                    android:layout_alignStart="@id/appNewsHeading"
                    android:paddingStart="12dp"
                    android:paddingEnd="5dp"
                    android:paddingBottom="4dp"
                    android:text="Click here to view latest info about the app"
                    android:textSize="12sp"
                    app:layout_constraintEnd_toStartOf="@id/appNewsChevron"
                    app:layout_constraintStart_toEndOf="@id/appNewsImage"
                    app:layout_constraintTop_toBottomOf="@id/appNewsHeading" />

                <ImageView
                    android:id="@+id/appNewsChevron"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginStart="12dp"
                    android:layout_marginEnd="10dp"
                    android:src="@drawable/ic_baseline_chevron_right_24"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="ContentDescription" />
            </androidx.constraintlayout.widget.ConstraintLayout>
        </com.google.android.material.card.MaterialCardView>

        <com.google.android.material.card.MaterialCardView
            style="?attr/materialCardViewElevatedStyle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="5dp"
            app:cardCornerRadius="28dp"
            app:cardPreventCornerOverlap="false"
            app:cardUseCompatPadding="true"
            android:visibility="gone">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/constraintVersionInfo"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:foreground="?attr/selectableItemBackground"
                tools:targetApi="m"
                tools:ignore="UnusedAttribute">

                <ImageView
                    android:id="@+id/versioninfoImage"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginStart="12dp"
                    android:src="@drawable/ic_baseline_info_24"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="ContentDescription" />

                <TextView
                    android:id="@+id/versionInfoHeading"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:paddingStart="12dp"
                    android:paddingTop="4dp"
                    android:paddingEnd="5dp"
                    android:text="Version Info"
                    android:textSize="18sp"
                    app:layout_constraintEnd_toStartOf="@id/versionInfoChevron"
                    app:layout_constraintStart_toEndOf="@id/versioninfoImage"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/versionInfoHeading"
                    android:layout_alignStart="@id/versionInfoHeading"
                    android:paddingStart="12dp"
                    android:paddingEnd="5dp"
                    android:paddingBottom="4dp"
                    android:text="View info about this version of the app"
                    android:textSize="12sp"
                    app:layout_constraintEnd_toStartOf="@id/versionInfoChevron"
                    app:layout_constraintStart_toEndOf="@id/versioninfoImage"
                    app:layout_constraintTop_toBottomOf="@id/versionInfoHeading" />

                <ImageView
                    android:id="@+id/versionInfoChevron"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginStart="12dp"
                    android:layout_marginEnd="10dp"
                    android:src="@drawable/ic_baseline_chevron_right_24"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    tools:ignore="ContentDescription" />
            </androidx.constraintlayout.widget.ConstraintLayout>
        </com.google.android.material.card.MaterialCardView>
    </LinearLayout>
    </androidx.core.widget.NestedScrollView>

</androidx.coordinatorlayout.widget.CoordinatorLayout>